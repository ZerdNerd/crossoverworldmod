add_namespace = enchancian_cw_ai

country_event = {
	id = enchancian_cw_ai.1
	immediate = {
		log = "[GetDateText]: [Root.GetName]: event enchancian_cw_ai.1"
	}
	title = enchancian_cw_ai.1.t
	desc = enchancian_cw_ai.1.d
	picture = GFX_report_enchancia_palace
	
	trigger = {
		has_global_flag = enchancian_civil_war
		NOT = {
			has_global_flag = enchancian_civil_war_over
		}
		tag = ENC
		is_ai = yes
		EN1 = {
			exists = yes
			OR = {
				is_puppet_of = ENC
				NOT = {
					has_war_with = ENC
				}
			}
		}
	}
	
	option = { #fix this
		name = enchancian_cw_ai.1.a
		hidden_effect = {
			ENC = {
				annex_country = {
					target = EN1
					transfer_troops = no
				}
			}
		}
	}
}

